<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_yacht"
    app:startDestination="@id/yachtListFragment">
    <fragment
        android:id="@+id/yachtListFragment"
        android:name="com.shareyacht.shareyacht.YachtListFragment"
        android:label="fragment_yacht_list"
        tools:layout="@layout/fragment_yacht_list" >
        <action
            android:id="@+id/action_yachtListFragment_to_yachtDetailFragment"
            app:destination="@id/yachtDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/yachtDetailFragment"
        android:name="com.shareyacht.shareyacht.YachtDetailFragment"
        android:label="fragment_yacht_detail"
        tools:layout="@layout/fragment_yacht_detail" >
        <action
            android:id="@+id/action_yachtDetailFragment_to_yachtReserveFragment"
            app:destination="@id/yachtReserveFragment" />
    </fragment>
    <fragment
        android:id="@+id/yachtReserveFragment"
        android:name="com.shareyacht.shareyacht.YachtReserveFragment"
        android:label="fragment_yacht_reserve"
        tools:layout="@layout/fragment_yacht_reserve" />
</navigation>